{
  "name": "naabu_sveltekit",
  "version": "0.0.1",
  "license": "GPLv2",
  "scripts": {
    "dev": "node scripts/generateClientConfig.js; vite dev --host --port 3000 ",
    "build": "bash scripts/build.sh",
    "preview": "svelte-kit preview",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "addUserClaims": "export GOOGLE_APPLICATION_CREDENTIALS=/var/www/adminkey.json; node scripts/addUserClaims.js",
    "emulate": "node scripts/generateServerConfig.js; firebase emulators:start  --import=./data/.devdata --export-on-exit --only auth,functions,firestore,pubsub",
    "emulateCypress": "node scripts/generateServerConfig.js; firebase emulators:start --only auth,functions,firestore,pubsub",
    "playwright": "npx playwright test --project=localhost src",
    "playwright-docker": "npx playwright test --project=docker src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs/references/storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@playwright/test": "^1.20.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/builder-webpack4": "^6.4.19",
    "@storybook/source-loader": "^6.4.19",
    "@storybook/svelte": "^6.4.19",
    "@sveltejs/kit": "1.0.0-next.405",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "cssnano": "^5.1.5",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "firebase-admin": "^10.0.2",
    "firebase-tools": "^10.5.0",
    "json-ptr": "^3.0.1",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.3",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "svelte": "^3.46.2",
    "svelte-adapter-firebase": "^0.14.0",
    "svelte-knobby": "^0.3.4",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.4",
    "sveltedoc-parser": "^4.2.1",
    "tailwindcss": "^3.0.12",
    "vite": "^3.0.9"
  },
  "type": "module",
  "dependencies": {
    "@algolia/autocomplete-js": "^1.5.4",
    "@algolia/autocomplete-theme-classic": "^1.5.4",
    "@algolia/client-search": "^4.13.0",
    "@fontsource/fira-mono": "^4.5.6",
    "@lukeed/uuid": "^2.0.0",
    "@sveltejs/pancake": "0.0.18",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.4.1",
    "@vimeo/player": "^2.16.4",
    "algoliasearch": "^4.13.0",
    "cookie": "^0.4.1",
    "diff-match-patch": "^1.0.5",
    "dompurify": "^2.3.6",
    "firebase": "^8.0.0",
